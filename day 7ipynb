   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "b36ac6e2-8bac-4904-ae5a-d922dca9f31e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'a': 2, 'b': 3, 'c': 4, 'd': 5, 'e': 6, 'f': 7}\n"
